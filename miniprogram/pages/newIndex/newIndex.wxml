<!-- 轮播图 -->
<swiper autoplay indicator-dots interval='2000' circular='true'>
<swiper-item wx:for="{{imageList}}" wx:key="id">
<image src="{{item.image}}" mode='widthfix'></image>
</swiper-item>
<!-- <swiper-item>
<image src="/images/banner-02.png" mode='widthfix'></image>
</swiper-item> -->
</swiper>
<!-- 九宫格 分类 -->
<view class='outer'>
  <navigator wx:for="{{categories}}" wx:key="id" url="{{item.url}}" class='item'>
  <image src="{{item.icon}}"></image>
  <text>{{item.name}}</text>
  </navigator>
</view>
